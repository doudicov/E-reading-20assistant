{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "Book",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "bookid": {
      "type": "uid",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "author": {
      "type": "string"
    },
    "date_published": {
      "type": "date"
    },
    "date_added": {
      "type": "datetime"
    },
    "description": {
      "type": "text"
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "books"
    },
    "book_content": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::book-content.book-content",
      "mappedBy": "book"
    },
    "book_cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "bookuuid": {
      "type": "customField",
      "customField": "plugin::strapi-advanced-uuid.uuid"
    },
    "user_book_progresses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::user-book-progress.user-book-progress",
      "mappedBy": "books"
    },
    "price": {
      "type": "float",
      "default": 10,
      "required": true
    },
    "purchases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-book-purchase.user-book-purchase",
      "mappedBy": "book"
    }
  }
}
